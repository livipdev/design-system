{
  "animations": {
    "outlinedButton": {
      "root": {
        "overflow": "hidden",
        "position": "relative"
      },
      "after": {
        "height": "155px",
        "left": "-75px",
        "opacity": "0.2",
        "position": "absolute",
        "top": "-50px",
        "transform": "rotate(35deg)",
        "transition": "all 550ms cubic-bezier(0.19, 1, 0.22, 1)",
        "width": "50px",
        "zIndex": 1
      },
      "hover": {
        "left": "120%",
        "transition": "all 550ms cubic-bezier(0.19, 1, 0.22, 1)"
      }
    }
  }
}
